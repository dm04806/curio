<li class="media box">
  <a class="pull-left" href="">
    <img class="qrcode" src="{{qrcode_url}}" alt="">
  </a>
  <div class="media-body">
    <div class="ops">
      <a href="{{url 'channel#show' id}}">
        <i class="icon-file"></i> {{t 'detail'}}
      </a>
      <a href="" data-op="destroyOne" data-id="{{id}}">
        <i class="icon-remove"></i>{{t 'delete'}}
      </a>
    </div>
    <h5 class="title">
      <span class="text-muted scene-id">
        {{scene_id}}
      </span>
      {{name}}
    </h5>
  </div>
</li>
