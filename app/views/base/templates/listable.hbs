<table class="table {{tableClass}}">
  {{include thead}}
  <tbody class="list">
  </tbody>
</table>
<div class="placeholder">
  <div class="loading-indicator spinner spinner30">
    <!--{{t 'loading'}}-->
  </div>
</div>
<div class="fallback">
  {{#if fallback.title}}
    <h2>{{t fallback.title}}</h2>
  {{/if}}
  {{#if fallback.message}}
    <p>{{t fallback.message}}</p>
  {{/if}}
</div>
